<%= form_with url: sessions_path, local: true do |form| %>

  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email, id: :email %><br>
  </p>

  <p>
    <%= form.label :password %><br>
    <%= form.password_field :password, id: :password%><br>
  </p>

  <p><%= form.submit 'Enter'%></p>
<% end %>
